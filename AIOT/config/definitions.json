{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password_hash": "admin",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "internal_cluster_id",
      "value": "rabbitmq-cluster-id-aiot"
    }
  ],
  "policies": [],
  "queues": [
    {
      "name": "task.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 10000
      }
    },
    {
      "name": "notification.queue",
      "vhost": "/", 
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000
      }
    },
    {
      "name": "sensor.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 50000
      }
    }
  ],
  "exchanges": [
    {
      "name": "iot_exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "iot_exchange",
      "vhost": "/",
      "destination": "task.queue",
      "destination_type": "queue",
      "routing_key": "task.#",
      "arguments": {}
    },
    {
      "source": "iot_exchange", 
      "vhost": "/",
      "destination": "notification.queue",
      "destination_type": "queue",
      "routing_key": "notification.#",
      "arguments": {}
    },
    {
      "source": "iot_exchange",
      "vhost": "/",
      "destination": "sensor.queue",
      "destination_type": "queue",
      "routing_key": "sensor.#",
      "arguments": {}
    }
  ]
}